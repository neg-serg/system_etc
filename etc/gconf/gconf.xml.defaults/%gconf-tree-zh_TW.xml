<?xml version="1.0"?>
<gconf>
	<dir name="schemas">
		<dir name="apps">
			<dir name="docky-2">
				<dir name="WorkspaceSwitcher">
					<dir name="WorkspaceSwitcherDockItem">
						<entry name="WrappedScrolling">
							<local_schema>
								<longdesc>如果值為真，便能以單一方向捲動至所有工作區。</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="WeatherDocklet">
					<dir name="WeatherPreferences">
						<entry name="Timeout">
							<local_schema short_desc="多久檢查一次">
								<longdesc>多久檢查一次天氣資訊。</longdesc>
							</local_schema>
						</entry>
						<entry name="Source">
							<local_schema short_desc="天氣資訊來源">
								<longdesc>取得天氣資訊的來源名稱。</longdesc>
							</local_schema>
						</entry>
						<entry name="Metric">
							<local_schema short_desc="使用公制單位">
								<longdesc>如果值為真，會以公制單位顯示天氣。</longdesc>
							</local_schema>
						</entry>
						<entry name="Location">
							<local_schema short_desc="天氣地點">
								<longdesc>要檢查的天氣地點清單。</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="Timer">
					<dir name="TimerMainDockItem">
						<entry name="DismissOnClick">
							<local_schema short_desc="當點擊時刪除已停止的計時器">
								<longdesc>如果失敗，倒數計時器會重設而不是結束</longdesc>
							</local_schema>
						</entry>
						<entry name="DefaultTimer">
							<local_schema short_desc="新計時器的長度">
								<longdesc>新計時器的預設時間 (秒鐘)。</longdesc>
							</local_schema>
						</entry>
						<entry name="AutoStartTimers">
							<local_schema short_desc="開始全部的新計時器">
								<longdesc>如果值為真，所有新的計時器都會自動開始。</longdesc>
							</local_schema>
						</entry>
						<entry name="AutoDismissTimers">
							<local_schema short_desc="清除已完成的計時器">
								<longdesc>如果值為真，計時器會在計時結束後消失。</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="SessionManager">
					<dir name="SessionManagerItem">
						<entry name="CurrentIndex">
							<local_schema short_desc="目前選取的項目">
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="RecentDocuments">
					<dir name="RecentDocumentsItem">
						<entry name="NumRecentDocs">
							<local_schema short_desc="最近開啓文件數目">
							</local_schema>
						</entry>
						<entry name="AlwaysShowRecent">
							<local_schema short_desc="顯示最近使用過的文件">
								<longdesc>如果值為真，將會在 Dock 上顯示最近使用過的文件。</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="NPR">
					<dir name="NPR">
						<entry name="MyStations">
							<local_schema short_desc="NPR 站台">
								<longdesc>要顯示的 NPR 電臺清單。</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="GMail">
					<dir name="GMailPreferences">
						<entry name="User">
							<local_schema short_desc="GMail 使用者名稱">
								<longdesc>要查看的 GMail 使用者名稱。</longdesc>
							</local_schema>
						</entry>
						<entry name="RefreshRate">
							<local_schema short_desc="多久檢查一次">
								<longdesc>多久 (分鐘) 檢查一次新郵件。</longdesc>
							</local_schema>
						</entry>
						<entry name="Notify">
							<local_schema short_desc="顯示未讀信件通知">
								<longdesc>如果值為真，幫新的未讀信件顯示通知訊息。</longdesc>
							</local_schema>
						</entry>
						<entry name="NeedsAttention">
							<local_schema short_desc="顯示未讀郵件指示器的點標記">
								<longdesc>如果值為真，當有未讀信件時，Docky 會顯示出指示標記。</longdesc>
							</local_schema>
						</entry>
						<entry name="LastChecked">
							<local_schema short_desc="上次檢查時間">
								<longdesc>Docky 最後一次檢查 GMail 訊息的時間。</longdesc>
							</local_schema>
						</entry>
						<entry name="Labels">
							<local_schema short_desc="要檢查的 GMail 標籤">
								<longdesc>除了收信匣外要查看的自訂 GMail 標籤清單。</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="Docky">
					<dir name="Services">
						<dir name="ThemeService">
							<entry name="UrgentHue">
								<local_schema short_desc="緊急指示器顏色變換">
									<longdesc>要變更多少緊急指示器的顏色。數值可以從-180到180。</longdesc>
								</local_schema>
							</entry>
							<entry name="Theme">
								<local_schema short_desc="目前使用主題">
									<longdesc>Docky 目前主題的名稱。</longdesc>
								</local_schema>
							</entry>
							<entry name="GlowTime">
								<local_schema short_desc="緊急指示器的發光動畫時間 (秒鐘)">
									<longdesc>緊急指示器的發光動畫時間多久？(秒鐘)，0 為關閉，-1 為永久顯示。</longdesc>
								</local_schema>
							</entry>
						</dir>
						<dir name="HelperService">
							<entry name="ShowOutput">
								<local_schema short_desc="將輔助程式的輸出重新導引至 Docky 輸出">
								</local_schema>
							</entry>
						</dir>
					</dir>
					<dir name="Items">
						<dir name="WnckDockItem">
							<entry name="ShowHovers">
								<local_schema short_desc="滑鼠游標移到啟動圖示時，顯示文字標籤">
									<longdesc>若為否，Docky 將不會為啟動器顯示滑鼠指標懸停時的文字 (但仍會為插件顯示)。</longdesc>
								</local_schema>
							</entry>
							<entry name="CurrentDesktopOnly">
								<local_schema short_desc="只顯示目前桌面上的視窗">
									<longdesc>如果值為真，Docky 只會展現目前使用中的桌面區域視窗而已。</longdesc>
								</local_schema>
							</entry>
						</dir>
						<dir name="DockyItem">
							<entry name="ShowSettings">
								<local_schema short_desc="讓 Docky 項目選單出現「設定值」">
									<longdesc>如果值為真，Docky 的彈出式選單會有「設定值」選項。</longdesc>
								</local_schema>
							</entry>
							<entry name="ShowQuit">
								<local_schema short_desc="讓 Docky 項目選單出現「退出」">
									<longdesc>如果值為真，Docky 的彈出式選單會有「退出」的選項。</longdesc>
								</local_schema>
							</entry>
							<entry name="ShowDockyItem">
								<local_schema short_desc="顯示 Docky 項目">
									<longdesc>如果值為真，在 Dock 管理視窗上面展現所有 Docky 的項目。</longdesc>
								</local_schema>
							</entry>
							<entry name="Hue">
								<local_schema short_desc="Docky 項目色調">
									<longdesc>設定 Docky 圖示的顏色。0 代表使用 GTK+ 主題的顏色。</longdesc>
								</local_schema>
							</entry>
							<entry name="HoverText">
								<local_schema short_desc="Docky 項目懸停文字">
								</local_schema>
							</entry>
							<entry name="DockyItemCommand">
								<local_schema short_desc="要執行的 Docky 項目指令">
								</local_schema>
							</entry>
						</dir>
					</dir>
					<dir name="Interface">
						<dir name="DockPreferences">
							<entry name="FirstRun">
								<local_schema short_desc="不要更動它！">
									<longdesc>如果值為真，代表這是 Docky 第一次執行。開發小組專用，不要改變這個數值！</longdesc>
								</local_schema>
							</entry>
							<dir name="DockN">
								<entry name="ZoomPercent">
									<local_schema short_desc="縮放尺寸">
										<longdesc>Dock 放大的尺寸 (如果有開啓的話)，以百分比計算。</longdesc>
									</local_schema>
								</entry>
								<entry name="ZoomEnabled">
									<local_schema short_desc="Dock 縮放">
										<longdesc>如果值為真，當滑鼠指標懸停時，Dock 會放大。</longdesc>
									</local_schema>
								</entry>
								<entry name="WindowManager">
									<local_schema short_desc="Dock 管理所有視窗">
										<longdesc>如果值為真，所有沒放啟動圖示在 dock 上的視窗都會展現在這個 dock 上。只能有一個 dock 將這個值設為真。</longdesc>
									</local_schema>
								</entry>
								<entry name="ThreeDimensional">
									<local_schema short_desc="Dock 為 3d">
										<longdesc>如果值為真，Dock 會以 3D 的方式展現。</longdesc>
									</local_schema>
								</entry>
								<entry name="SortList">
									<local_schema short_desc="為 Dock 上的項目分類">
										<longdesc>使用來為 dock 上的項目分類。</longdesc>
									</local_schema>
								</entry>
								<entry name="Position">
									<local_schema short_desc="Dock 的位置">
										<longdesc>Dock 在螢幕上的位置。可能的選項有：底部，頂部，左邊，右邊。</longdesc>
									</local_schema>
								</entry>
								<entry name="Plugins">
									<local_schema short_desc="此 Dock 上的插件">
										<longdesc>此 Dock 上使用中的插件清單。</longdesc>
									</local_schema>
								</entry>
								<entry name="PanelMode">
									<local_schema short_desc="Dock 是否當作面板">
										<longdesc>如果值為真，Docky 會進入面板模式，並延伸到整個螢幕邊緣。</longdesc>
									</local_schema>
								</entry>
								<entry name="Launchers">
									<local_schema short_desc="啟動圖示清單">
									</local_schema>
								</entry>
								<entry name="IndicateMultipleWindows">
									<local_schema short_desc="當項目超過一個視窗時明確指出">
									</local_schema>
								</entry>
								<entry name="IconSize">
									<local_schema short_desc="Dock 圖示大小">
										<longdesc>Dock 上的圖示大小。</longdesc>
									</local_schema>
								</entry>
								<entry name="HotAreaPadding">
									<local_schema short_desc="填補熱門區域">
									</local_schema>
								</entry>
								<entry name="FadeOpacity">
									<local_schema short_desc="淡化透明度">
										<longdesc>若要在隱藏時淡出，這是 Docky 淡出時的透明度。1 代表不透明，0 代表透明。</longdesc>
									</local_schema>
								</entry>
								<entry name="FadeOnHide">
									<local_schema short_desc="當隱藏時漸淡">
										<longdesc>如果值為真，Dock 隱藏時會淡出，而不是滑出螢幕畫面。</longdesc>
									</local_schema>
								</entry>
								<entry name="Autohide">
									<local_schema short_desc="Dock 的隱藏模式">
									</local_schema>
								</entry>
							</dir>
						</dir>
						<dir name="DockDragTracker">
							<entry name="ProvidersAcceptDrops">
								<local_schema short_desc="拒絕拖曳新增項目">
									<longdesc>如果失敗，阻止任何項目丟入、新增到 dock 上。</longdesc>
								</local_schema>
							</entry>
							<entry name="LockDrags">
								<local_schema>
									<longdesc>如果值為真，鎖定所有 Dock 上的項目，讓它們無法被拖曳。</longdesc>
								</local_schema>
							</entry>
						</dir>
						<dir name="AutohideManager">
							<entry name="UpdateDelay">
								<local_schema short_desc="視窗切換之後多久才更新 (毫秒)">
								</local_schema>
							</entry>
							<entry name="UnhideDelay">
								<local_schema short_desc="取消隱藏前需等待的時間 (毫秒)">
								</local_schema>
							</entry>
						</dir>
					</dir>
					<dir name="DockController">
						<entry name="CompositeCheckEnabled">
							<local_schema short_desc="啟用混成檢查">
								<longdesc>如果值為真，Docky 會檢查混成能力，並且在無法執行的時候提出警告。</longdesc>
							</local_schema>
						</entry>
						<entry name="ActiveDocks">
							<local_schema short_desc="使用中 Dock 的清單">
								<longdesc>這會依照名稱列出目前使用中 (顯示) 的 Dock。</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="CPUMonitor">
					<dir name="CPUMonitorDockItem">
						<entry name="ClickCommand">
							<local_schema short_desc="滑鼠左鍵指令">
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="Clock">
					<dir name="ClockDockItem">
						<entry name="ShowMilitary">
							<local_schema short_desc="使用 24 小時制">
								<longdesc>如果值為真，將時間顯示成 24 小時制</longdesc>
							</local_schema>
						</entry>
						<entry name="ShowDigital">
							<local_schema short_desc="顯示數位時鐘">
								<longdesc>如果值為真，顯示數位時鐘，否則顯示類比時鐘。</longdesc>
							</local_schema>
						</entry>
						<entry name="ShowDate">
							<local_schema short_desc="顯示日期">
								<longdesc>如果值為真，數位時鐘上會有日期。</longdesc>
							</local_schema>
						</entry>
						<entry name="ClockTheme">
							<local_schema short_desc="類比時鐘主題">
								<longdesc>類比時鐘的主題。</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="Clippy">
					<dir name="ClippyItem">
						<entry name="TrackMouseSelections">
							<local_schema short_desc="追蹤滑鼠選取項目">
							</local_schema>
						</entry>
						<entry name="TimerDelay">
							<local_schema short_desc="計時器延遲">
								<longdesc>多久更新一次剪貼簿資料 (毫秒)</longdesc>
							</local_schema>
						</entry>
						<entry name="MaxEntries">
							<local_schema short_desc="最大條目數">
								<longdesc>顯示多少剪貼簿的內容</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
			</dir>
		</dir>
	</dir>
</gconf>
